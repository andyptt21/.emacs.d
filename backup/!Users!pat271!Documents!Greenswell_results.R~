library(ggplot2)
library(cowplot)
data<-read.csv("~/Documents/Greenswell_results.csv")

swim<-ggplot(data,aes(x=Swim.Points)) +
    geom_histogram(fill="blue",binwidth = 5) +
    geom_vline(xintercept=median(data$Swim.Points),lty = 2) +
    theme_classic() +
    geom_vline(xintercept=44)
bike<-ggplot(data,aes(x=Bike.Points)) +
    geom_histogram(fill="forestgreen",binwidth = 5) +
    geom_vline(xintercept=median(data$Bike.Points),lty = 2) +
    theme_classic() +
    geom_vline(xintercept=19.5)
run<-ggplot(data,aes(x=Run.Points)) +
    geom_histogram(fill="red",binwidth = 5) +
    geom_vline(xintercept=median(data$Run.Points),lty = 2) +
    theme_classic() +
    geom_vline(xintercept=35)
age<-ggplot(data,aes(x=Age)) +
    geom_histogram(fill="goldenrod",binwidth = 5) +
    geom_vline(xintercept=median(data$Age),lty = 2) +
    theme_classic() +
    geom_vline(xintercept=25)
plot_grid(plotlist=list(age,swim,bike,run),ncol=2)
