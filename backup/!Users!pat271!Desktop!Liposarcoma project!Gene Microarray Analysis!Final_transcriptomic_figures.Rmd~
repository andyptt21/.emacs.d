all<-intersect(chib_tcga,dent_tcga)

overlap<-data.frame(Transcript=c(all,chib_tcga,chib_dent,dent_tcga),
                    Datasets=c(3,
                               rep(2,length(chib_tcga)),
                               rep(2,length(chib_dent)),
                               rep(2,length(dent_tcga))))
