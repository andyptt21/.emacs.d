chibon_fishers<-runCombinedFisherTest(pathwaydf = chibon_pathways,conpass = "")
save(chibon_fishers, MDM2_correlations_chibon_sig, MDM2_corr_test_chibon, chibon_data_exprs, plotlist, UGCG_chibon, pval_plot_chibon, file="chibon_RaMP_analysis.Rda")

