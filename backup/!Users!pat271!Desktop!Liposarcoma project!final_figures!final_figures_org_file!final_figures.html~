ggplot(resMDM2HiLoSig,aes(x=reorder(name,log2fc), y=log2fc, fill = Class)) +
    ggtitle(<span style="color: #2d9574;">"Significant MDM2 Hi/Lo Metabolites"</span>) +
    geom_bar(stat = <span style="color: #2d9574;">"identity"</span>, colour = <span style="color: #2d9574;">"black"</span>) +
    coord_flip() +
    theme_bw() +
    ylab(<span style="color: #2d9574;">"log2(mean(MDM2hi)-mean(MDM2lo))"</span>) +
    theme(axis.title.y = element_blank(),axis.title.x = element_text(size=12,face=<span style="color: #2d9574;">"bold"</span>),axis.text.y=element_text(size=10,face=<span style="color: #2d9574;">"bold"</span>),axis.text.x=element_text(size=10)) +
        theme(panel.grid.major = element_blank(), panel.grid.minor = element_blank()) +
    scale_fill_manual(values=as.vector(myPalette)[c(1,2,5,3,4,6,7)]) +
    theme(legend.position=<span style="color: #2d9574;">"bottom"</span>,legend.direction=<span style="color: #2d9574;">"vertical"</span>)
