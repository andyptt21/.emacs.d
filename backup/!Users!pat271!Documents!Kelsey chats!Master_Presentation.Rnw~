textsByMonth<-paste(chatLogs$Year,chatLogs$Month,sep="-")
textsByMonth<-data.frame(table(textsByMonth))
colnames(textsByMonth)=c("Month","Freq")

labels<-unique(paste(month.abb[as.numeric(chatLogs$Month)],gsub("20","'",chatLogs$Year),sep=" "))

fill=c(rep("red",8),
       rep("orange",4),
       rep("navyblue",8),
       rep("darkorange",15),
       rep("forestgreen",18),
       rep("purple",9))

relationship_timeline<-ggplot(textsByMonth, aes(x=Month,y=Freq)) +
    geom_rect(data = textsByMonth,
              xmin = as.numeric(textsByMonth$Month[[1]]) - 0.5,
              xmax = as.numeric(textsByMonth$Month[[8]]) + 0.5,
              ymin = 0, ymax = 6000, alpha=0.5,fill="pink") +
    geom_rect(data = textsByMonth,
              xmin = as.numeric(textsByMonth$Month[[9]]) - 0.5,
              xmax = as.numeric(textsByMonth$Month[[12]]) + 0.5,
              ymin = 0, ymax = 6000, alpha=0.5,fill="yellow") +
    geom_rect(data = textsByMonth,
              xmin = as.numeric(textsByMonth$Month[[13]]) - 0.5,
              xmax = as.numeric(textsByMonth$Month[[20]]) + 0.5,
              ymin = 0, ymax = 6000, alpha=0.5,fill="lightblue") +
    geom_rect(data = textsByMonth,
              xmin = as.numeric(textsByMonth$Month[[21]]) - 0.5,
              xmax = as.numeric(textsByMonth$Month[[35]]) + 0.5,
              ymin = 0, ymax = 6000, alpha=0.5,fill="gold") +
    geom_rect(data = textsByMonth,
