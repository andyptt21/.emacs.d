<<include=FALSE>>=
library(network)
textsByMonth<-paste(chatLogs$Year,chatLogs$Month,sep="-")
textsByMonth<-as.color(textsByMonth)

juniorYear<-chatLogs[which(textsByMonth %in% 1:8),]

set.seed(1234)

naughty_words_list<-c("sex","sexy",
                      "fuck","fucking","fucked",
                      "high","damn","shit","shitty","dick","ass",
                      "http","tenor","keyboard","sticker","photo","sent","just")

library("tm")
library("SnowballC")
library("wordcloud")
library("RColorBrewer")
library("hunspell")

wordMatrixGenerator<-function(messages){
    docs<-Corpus(VectorSource(unlist(messages)))
    toSpace <- content_transformer(function (x , pattern ) gsub(pattern, " ", x))
    docs <- tm_map(docs, toSpace, "/")
    docs <- tm_map(docs, toSpace, "@")
    docs <- tm_map(docs, toSpace, "\\|")
    docs <- tm_map(docs, content_transformer(tolower))
    docs <- tm_map(docs, removeNumbers)
    docs <- tm_map(docs, removeWords, stopwords("english"))
    docs <- tm_map(docs, removePunctuation)
    docs <- tm_map(docs, stripWhitespace)
    dtm <- TermDocumentMatrix(docs)
    m <- as.matrix(dtm)
    v <- sort(rowSums(m),decreasing=TRUE)
    d <- data.frame(word = names(v),freq=v)
    index <- match(naughty_words_list,rownames(d))
    index <- index[!is.na(index)]
    d <- d[-index,]
    correct<-hunspell_check(rownames(d))
    d<-d[correct,]
    d <- within(d,
            word <- factor(word,
                           levels=unique(word)))
    return(d)
}

word_tracker<-function(word,color="white",
                       month_range=c(1:length(unique(textsByMonth))),
                       title=paste0("Uses of the word '",word,"'")){
    word_found<-sapply(chatLogs$Message, function(x)
        return(grepl(word,x,ignore.case=TRUE)))
    word_distribution<-textsByMonth[word_found]
    word_frequency<-table(word_distribution)
    word_distribution<-rep(0,length(unique(textsByMonth)))
    names(word_distribution)<-1:length(unique(textsByMonth))
    word_distribution[names(word_frequency)]<-word_frequency
    word_df<-data.frame(Month=1:length(unique(textsByMonth)),Freq=word_distribution)
    word_df<-word_df[month_range,]
    word_plot<-ggplot(word_df, aes(x=Month,y=Freq)) +
        geom_bar(colour="black", fill=color,stat="identity") +
        theme_classic() +
        theme(plot.title = element_text(hjust = 0.5)) +
        theme(text = element_text(size=20),
              legend.title=element_blank(),
              axis.text.x=element_text(angle = -90, hjust = 0.5,vjust = 0.5)) +
        scale_x_continuous(labels=rev(labels)[month_range],breaks=month_range,
                           expand=c(0,0)) +
        ggtitle(title) +
        labs(x="", y = "Count")
    return(word_plot)
}

word_database<-function(word){
    word_found<-sapply(chatLogs$Message, function(x)
        return(grepl(word,x,ignore.case=TRUE)))
    return(chatLogs[word_found,])
}

