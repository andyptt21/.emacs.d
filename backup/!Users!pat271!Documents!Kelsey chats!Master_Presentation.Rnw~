<<Making dataset,eval=F,echo=FALSE>>=
library(rjson)
library(anytime)

chatLogs<-fromJSON(file="~/Documents/Kelsey chats/KelseyScott_3b97801735/message.json")
chatLogs<-do.call(rbind,chatLogs$messages)

chatLogs<-chatLogs[,-c(4:6)]
times<-sapply(chatLogs[,2], function(x) return(as.character(anytime(as.numeric(x)/1000))))
times<-sapply(times,strsplit, split=" ")
times<-do.call(rbind,times)
dates<-sapply(times[,1],strsplit, split="-")
dates<-do.call(rbind,dates)

chatLogs<-chatLogs[,-2]
chatLogs<-cbind(chatLogs,dates,times[,2])
colnames(chatLogs)<-c("Sender","Message","Year","Month","Day","Time")
chatLogs<-as.data.frame(chatLogs)

save(chatLogs,file="~/Documents/Kelsey chats/chatLogs.Rda")
@

